{
    "name": "cakasim/payone-sdk",
    "description": "A SDK for PAYONE payment integration.",
    "type": "library",
    "keywords": [
        "payone",
        "payment",
        "sdk"
    ],
    "homepage": "https://cakasim.de",
    "readme": "README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabian BÃ¶ttcher",
            "email": "me@cakasim.de",
            "homepage": "https://cakasim.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@cakasim.de",
        "issues": "https://github.com/Cakasim/php-payone-sdk/issues",
        "source": "https://github.com/Cakasim/php-payone-sdk"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "psr/http-message": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/log": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Cakasim\\Payone\\Sdk\\": "src/"
        }
    },
    "suggest": {
        "ext-curl": "Required in order to use the cURL HTTP driver."
    },
    "require-dev": {
        "slim/psr7": "^0.6.0"
    },
    "scripts": {
        "cakasim:download-phpdoc": "curl https://phpdoc.org/phpDocumentor.phar --output phpdoc",
        "cakasim:download-phpcsf": "curl https://cs.symfony.com/download/php-cs-fixer-v2.phar --output phpcs"
    },
    "scripts-descriptions": {
        "cakasim:download-phpdoc": "Downloads latest phpDocumentor PHAR and saves it as phpdoc.",
        "cakasim:download-phpcsf": "Downloads latest PHP Coding Standards Fixer and saves it as phpcsf."
    }
}
